package cliente;

public class PessoaJuridica extends Cliente{



	public PessoaJuridica(String nome, String profissao, int idade, int phone, String genero, String cnpj) {
		super(nome, profissao, idade, phone, genero);
		this.cnpj = cnpj;
		
		
		
	}
	private String cnpj;
	
	public String getCnpj() {
		return cnpj;
	}
	public void setCnpj(String cnpj) {
		this.cnpj = cnpj;
	}
	@Override
	public void visualizar() {
		super.visualizar();
		System.out.println("cnpj: " + cnpj);
	}
	

}
