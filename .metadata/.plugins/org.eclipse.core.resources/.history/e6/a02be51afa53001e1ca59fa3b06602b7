	   PessoaFisica novoCliente1 = new PessoaFisica();
	   PessoaJuridica novoCliente2 = new PessoaJuridica();
	   PessoaJuridica novoCliente3 = new PessoaJuridica();
	   
	   novoCliente.nome();
